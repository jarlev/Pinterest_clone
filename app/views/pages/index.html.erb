<h1>Hi! Welcome to Pinterest</h1><br>
	<% if user_signed_in? %>
	                <%= link_to "Sign out", destroy_user_session_path, method: :delete, class: "btn btn-danger" %>
	                <%= link_to "Edit profile", edit_user_registration_path, class: "btn btn-success" %>
	                <%= link_to "Create a new pin", new_pin_path, class: "btn btn-warning" %>
	              <%  else %>
	                <%= link_to "Sign in", new_user_session_path, class: "btn btn-info" %>
	                <%= link_to "Sign up", new_user_registration_path, class: "btn btn-primary" %>
	            <% end %>
